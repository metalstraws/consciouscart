{
  "expo": {
    "name": "concious-cart",
    "slug": "concious-cart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#9bce99"
    },
    "ios": {
      "supportsTablet": true,
      "statusBar": {
        "style": "light",
        "backgroundColor": "#9bce99"
      },
      "allowScreenCapture": true,
      "hideRuntimeDevMenu": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#9bce99"
      },
      "statusBar": {
        "backgroundColor": "#9bce99",
        "translucent": true,
        "style": "light"
      },
      "splash": {
        "backgroundColor": "#9bce99",
        "resizeMode": "contain",
        "image": "./assets/images/splash.png"
      },
      "softwareKeyboardLayoutMode": "pan",
      "allowScreenshots": true,
      "hideRuntimeDevMenu": true,
      "navigationBar": {
        "backgroundColor": "#9bce99",
        "visible": true
      },
      "package": "com.anonymous.conciouscart"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true,
    "developmentClient": {
      "silentLaunch": true
    }
  }
}
